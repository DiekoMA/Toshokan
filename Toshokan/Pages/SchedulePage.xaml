<Page x:Class="Toshokan.Pages.SchedulePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Toshokan.Pages"
      mc:Ignorable="d"
      xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
      xmlns:hc="https://handyorg.github.io/handycontrol"
      MinHeight="450" MinWidth="800">
    
    <DockPanel>
        <DockPanel DockPanel.Dock="Top" LastChildFill="False">
            <Button Content="{iconPacks:BoxIcons Kind=RegularLeftArrow}" DockPanel.Dock="Left"/>
            <TextBlock Text="Summer 2019" VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <Button Content="{iconPacks:BoxIcons Kind=RegularRightArrow}" DockPanel.Dock="Right"/>
        </DockPanel>
        <ListView>
            
        </ListView>
    </DockPanel>
</Page>
